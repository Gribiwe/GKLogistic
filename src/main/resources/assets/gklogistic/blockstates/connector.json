{
  "multipart": [
    {   "apply": { "model": "gklogistic:connector" }},
    {   "when": { "north": "true" },
      "apply": { "model": "gklogistic:connector_pipe", "uvlock": true }
    },
    {   "when": { "east": "true" },
      "apply": { "model": "gklogistic:connector_pipe", "y": 90, "uvlock": true }
    },
    {   "when": { "south": "true" },
      "apply": { "model": "gklogistic:connector_pipe", "y": 180, "uvlock": true }
    },
    {   "when": { "west": "true" },
      "apply": { "model": "gklogistic:connector_pipe", "y": 270, "uvlock": true }
    },
    {   "when": { "up": "true" },
      "apply": { "model": "gklogistic:connector_pipe", "y": 270, "x": 270, "uvlock": true }
    },
    {   "when": { "down": "true" },
      "apply": { "model": "gklogistic:connector_pipe", "y": 270, "x": 90, "uvlock": true }
    }

  ]
}
